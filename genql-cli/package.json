{
    "name": "genql-cli",
    "_": "[bump if genql-cli]",
    "version": "1.1.5",
    "description": "",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "mocha": {
        "require": "tests/init.js",
        "spec": "tests/**.ts",
        "timeout": 9999999999
    },
    "bin": {
        "genql": "./dist/cli.js"
    },
    "files": [
        "/dist/*",
        "/esm/*",
        "src"
    ],
    "scripts": {
        "start": "tsdx watch",
        "build": "tsc",
        "test": "jest",
        "compile": "sucrase -q ./src -d ./dist --transforms typescript,imports",
        "dev": "yarn compile && node dist"
    },
    "keywords": [],
    "author": "Tommaso De Rossi, morse <beats.by.morse@gmail.com>",
    "license": "ISC",
    "devDependencies": {
        "@sucrase/jest-plugin": "^2.0.0",
        "@types/common-tags": "^1.8.0",
        "@types/jest": "^23.3.13",
        "@types/listr": "^0.13.0",
        "@types/mkdirp": "^0.5.2",
        "@types/node": "^12.0.7",
        "@types/rimraf": "^2.0.2",
        "common-tags": "^1.8.0",
        "dotenv": "^8.2.0",
        "husky": "^1.3.1",
        "jest": ">=22 <24",
        "pretty-quick": "^1.10.0",
        "sucrase": "^3.12.1",
        "ts-jest": "^23.10.5",
        "typescript": "^3.9.5"
    },
    "dependencies": {
        "@graphql-tools/graphql-file-loader": "^6.0.10",
        "@graphql-tools/load": "^6.0.10",
        "@types/graphql": "^14.5.0",
        "@types/node": "*",
        "@types/prettier": "^1.19.0",
        "@types/yargs": "^15.0.5",
        "chalk": "^4.1.0",
        "graphql": "^15.1.0",
        "isomorphic-unfetch": "^3.0.0",
        "listr": "^0.14.3",
        "mkdirp": "^0.5.1",
        "prettier": "^1.19.1",
        "qs": "^6.9.1",
        "rimraf": "^2.6.3",
        "yargs": "^15.3.1"
    },
    "jest": {
        "transform": {
            ".(js|jsx|ts|tsx)": "@sucrase/jest-plugin"
        },
        "roots": [
            "<rootDir>/src"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
        "testEnvironment": "node",
        "collectCoverageFrom": [
            "src/**/*.ts",
            "!**/node_modules/**",
            "!src/_old/**",
            "!**/*.case.ts",
            "!src/testHelpers/**/*.ts"
        ],
        "coverageReporters": [
            "text"
        ]
    }
}
