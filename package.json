{
  "name": "gqlts",
  "version": "3.2.12",
  "private": true,
  "keywords": [
    "gqlts",
    "graphql",
    "axios",
    "typescript",
    "types-graphql",
    "types-graphql-client",
    "types-graphql-axios",
    "graphql-sdk",
    "graphql-sdk-generator",
    "graphql-sdk-cli",
    "graphql-subscriptions",
    "graphql-subscriptions-client",
    "graphql-typescript",
    "graphql-axios-client",
    "graphql-typescript-client",
    "graphql-typescript-sdk"
  ],
  "homepage": "https://gqlts.vercel.app",
  "repository": "github:meabed/gqlts",
  "license": "MIT",
  "author": {
    "name": "Mohamed Meabed",
    "email": "mo@meabed.com",
    "url": "https://meabed.com"
  },
  "maintainers": [
    {
      "name": "Mohamed Meabed",
      "email": "mo@meabed.com",
      "url": "https://meabed.com"
    }
  ],
  "workspaces": [
    "cli",
    "runtime",
    "try-clients",
    "website",
    "example-usage",
    "integration-tests"
  ],
  "scripts": {
    "buildall": "ultra -r --filter '@gqlts/*' --topology build",
    "examplecli": "yarn play -o example/generated -e https://graphql-compose.herokuapp.com/northwind/",
    "genall": "ultra -r gen",
    "integration": "cd integration-tests && yarn gen && tsc && yarn test",
    "play": "node -r sucrase/register cli/src/cli.ts",
    "prettier": "prettier --write \"cli/**/*.{ts,tsx,js,jsx}\" \"runtime/**/*.{ts,tsx,js,jsx}\" \"example-usage/**/*.{ts,tsx,js,jsx}\" \"integration-tests/**/*.{ts,tsx,js,jsx}\" \"try-clients/**/*.{ts,tsx,js,jsx}\"",
    "publ": "yarn buildall && lerna publish minor",
    "runexample": "node -r sucrase/register example/example.ts",
    "test": "ultra -r test",
    "tscall": "ultra -r --filter '@gqlts/*' tsc",
    "watch": "ultra -r watch"
  },
  "resolutions": {
    "graphql": "^16.6.0",
    "ws": "^8.9.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.0.0",
    "lerna": "6.3.0",
    "prettier": "2.8.1",
    "pretty-quick": "3.1.3",
    "typescript": "4.9.4",
    "ultra-runner": "3.10.5"
  }
}
