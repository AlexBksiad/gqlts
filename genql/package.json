{
    "name": "genql",
    "_": "[bump]",
    "version": "0.0.1",
    "description": "",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "mocha": {
        "require": "tests/init.js",
        "spec": "tests/**.ts",
        "timeout": 9999999999
    },
    "bin": {
        "generate-graphql-client": "./dist/cli.js"
    },
    "files": [
        "/dist/*",
        "/esm/*"
    ],
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "jest": "jest",
        "compile": "sucrase -q ./src -d ./dist --transforms typescript,imports",
        "dev": "yarn compile && node dist"
    },
    "keywords": [],
    "author": "Tommaso De Rossi, morse <beats.by.morse@gmail.com>",
    "license": "ISC",
    "devDependencies": {
        "@sucrase/jest-plugin": "^2.0.0",
        "@types/common-tags": "^1.8.0",
        "@types/jest": "^23.3.13",
        "@types/listr": "^0.13.0",
        "@types/mkdirp": "^0.5.2",
        "@types/node": "^12.0.7",
        "@types/rimraf": "^2.0.2",
        "common-tags": "^1.8.0",
        "dotenv": "^8.2.0",
        "husky": "^1.3.1",
        "jest": ">=22 <24",
        "pretty-quick": "^1.10.0",
        "sucrase": "^3.12.1",
        "ts-jest": "^23.10.5",
        "tsdx": "^0.13.2",
        "typescript": "^3.7.5"
    },
    "dependencies": {
        "@types/graphql": "^14.5.0",
        "@types/isomorphic-fetch": "^0.0.35",
        "@types/lodash": "^4.14.149",
        "@types/lodash.get": "^4.4.6",
        "@types/lodash.startswith": "^4.2.6",
        "@types/node": "*",
        "@types/prettier": "^1.19.0",
        "@types/qs": "^6.9.0",
        "@types/ws": "^6.0.1",
        "chalk": "^2.4.2",
        "commander": "^2.19.0",
        "graphql": "^14.5.8",
        "graphql-toolkit": "^0.3.0",
        "graphql-tools": "^4.0.6",
        "isomorphic-fetch": "^2.2.1",
        "listr": "^0.14.3",
        "lodash.get": "^4.4.2",
        "lodash.startswith": "^4.2.1",
        "mkdirp": "^0.5.1",
        "prettier": "^1.19.1",
        "qs": "^6.9.1",
        "rimraf": "^2.6.3",
        "rxjs": "^6.5.4",
        "subscriptions-transport-ws": "^0.9.16",
        "ws": "^6.1.4"
    },
    "jest": {
        "transform": {
            ".(js|jsx|ts|tsx)": "@sucrase/jest-plugin"
        },
        "roots": [
            "<rootDir>/src"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
        "testEnvironment": "node",
        "collectCoverageFrom": [
            "src/**/*.ts",
            "!**/node_modules/**",
            "!src/_old/**",
            "!**/*.case.ts",
            "!src/testHelpers/**/*.ts"
        ],
        "coverageReporters": [
            "text"
        ]
    }
}
