{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "noEmit": true,
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules",
    "../node_modules"
  ]
}
